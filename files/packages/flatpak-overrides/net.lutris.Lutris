[Context]
shared=network
sockets=pulseaudio
devices=all
filesystems=!/run/media;!/media;!xdg-data/flatpak;!xdg-desktop;!xdg-music;!xdg-videos;!xdg-pictures;~/Games/Lutris:create;xdg-documents/Gaming;xdg-pictures/Gaming;xdg-videos/Gaming/sandbox:create
persistent=WinePrefixes

[Environment]
PULSE_SINK=recording_out_sink
XDG_DOCUMENTS_DIR=/home/keks/Documents/Gaming
XDG_PICTURES_DIR=/home/keks/Pictures/Gaming
XDG_VIDEOS_DIR=/home/keks/Videos/Gaming/sandbox
